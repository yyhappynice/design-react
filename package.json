    "gulp-postcss": "^7.0.0",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^3.1.0",
    "html-webpack-plugin": "^2.30.1",
    "marked": "^0.3.6",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "pre-commit": "^1.2.2",
    "prismjs": "^1.8.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
